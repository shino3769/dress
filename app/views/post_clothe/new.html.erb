<h1>新規投稿フォーム</h1>
  <%= form_with model: @post_clothe do |f| %>
   
    <h4>投稿画像</h4>
      <%= f.file_field :image, accept: "image/*" %>
    <h4>タイトル</h4>
      <%= f.text_field :title %>
    <h4>ジャンル</h4>
      <%= f.text_field :genre %>
    <h4>説明</h4>
      <%= f.text_area :explanation %>
    <%= f.submit '投稿' %>
  <% end %>
  
  